{
    "extends": "next/core-web-vitals",
    "rules": {
        "no-unused-vars": "error",
        "prefer-const": ["error", { "ignoreReadBeforeAssign": true }],
        "no-unused-expressions": "error",
        "no-console": "warn",
        "no-undef": "error",
        "quotes": ["warn", "single", { "avoidEscape": true }]
    }
}
